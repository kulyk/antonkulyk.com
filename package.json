{
  "name": "blog",
  "private": true,
  "version": "1.0.0",
  "description": "Anton's Blog",
  "scripts": {
    "prettier": "prettier \"src/**/*.+(ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx .",
    "check-types": "tsc",
    "dev": "next",
    "run-script": "ts-node --project ./src/scripts/tsconfig.json",
    "generate-sitemap": "yarn run-script ./src/scripts/sitemap.ts",
    "generate-feed": "yarn run-script ./src/scripts/rss.tsx",
    "build": "next build",
    "export": "yarn build && next export && yarn generate-sitemap && yarn generate-feed",
    "analyze-bundle": "ANALYZE=true yarn build",
    "validate-project": "npm-run-all check-types check-format lint export"
  },
  "dependencies": {
    "@zeit/next-source-maps": "^0.0.3",
    "classnames": "^2.2.6",
    "csstype": "^3.0.5",
    "date-fns": "^2.16.1",
    "feed": "^4.2.1",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.17.0",
    "nextjs-sitemap-generator": "^1.1.3",
    "prismjs": "^1.22.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-console-emulator": "^3.0.5",
    "react-dom": "^17.0.1",
    "react-emoji-render": "^1.2.4",
    "react-markdown": "^5.0.3",
    "react-share": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@next/bundle-analyzer": "^10.0.3",
    "@types/classnames": "^2.2.11",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.16",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
